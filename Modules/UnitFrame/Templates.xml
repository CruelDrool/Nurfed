	<Frame name="Nurfed_Unit_Overlay" frameStrata="MEDIUM" movable="true" enableMouse="true" frameLevel="10" hidden="true" virtual="true" inherits="BackdropTemplate">
		<!--<Backdrop bgFile="Interface\Tooltips\UI-Tooltip-Background" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
			<BackgroundInsets left="5" right="4" top="4" bottom="5"/>
			<TileSize val="16"/>
			<EdgeSize val="16"/>
			<Color r="0" g="1" b="0" a="0.5"/>
			<BorderColor r="0.5" g="0.5" b="0" a="0"/>
		</Backdrop>-->
		<Layers>
			<Layer level="ARTWORK">
				<FontString font="GameFontNormal" parentKey="text">
					<Anchors>
						<Anchor point="TOP" y="-5" />
					</Anchors>
				</FontString>
				<FontString font="Nurfed_UnitFontSmallOutline" parentKey="placementCenter">
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				-- if BackdropTemplateMixin then
					-- Mixin(self, BackdropTemplateMixin)
				-- end
				self:SetBackdrop({
					bgFile = "Interface\\Tooltips\\UI-Tooltip-Background",
					edgeFile = "Interface\\Tooltips\\UI-Tooltip-Border",
					tile = true,
					tileEdge = true,
					tileSize = 16,
					edgeSize = 16,
					insets = { left = 5, right = 4, top = 4, bottom = 5 },
				})
				self:SetBackdropColor(0, 1, 0, 0.5)
				self:SetBackdropBorderColor(0.5, 0.5, 0, 0)
				
				self.text:SetText(self:GetParent():GetName())
				self:RegisterForDrag("LeftButton")
			</OnLoad>
			<OnEnter>
				self.enter = true
				if not self:GetParent().isMoving then
					self:SetBackdropBorderColor(0.5, 0.5, 0, 1)
				end
			</OnEnter>
			<OnMouseDown>
				if button == "LeftButton" then
					self:SetBackdropBorderColor(0, 0, 0, 0)
					local s = self:GetEffectiveScale()
					local x, y = GetCursorPosition()
					local xP, yP = self:GetParent():GetCenter()

					self.xOffset = (xP*s - x)/s
					self.yOffset = (yP*s - y)/s

					self.x = x / s + self.xOffset
					self.y = y / s + self.yOffset
					
					self.IsShiftKeyDown = IsShiftKeyDown()
					self.IsControlKeyDown = IsControlKeyDown()

					self:SetScript("OnUpdate",function(self, e)
					
						local x,y = GetCursorPosition()
						local s = self:GetEffectiveScale()
						x = x / s + self.xOffset
						y = y / s + self.yOffset

						if x ~= self.x and (not IsShiftKeyDown() and not self.IsShiftKeyDown) then
							self.x = x
						end
					
						if x ~= self.y and (not IsControlKeyDown() and not self.IsControlKeyDown) then
							self.y = y
						end

						self:GetParent():ClearAllPoints()
						self:GetParent():SetPoint("CENTER", UIParent, "BOTTOMLEFT", self.x, self.y)
					end)
				end

			</OnMouseDown>
			<OnMouseUp>
				self:SetScript("OnUpdate", nil)
				self:GetParent():SavePosition(parent)
				if button == "LeftButton" then
					self:SetBackdropBorderColor(0.5, 0.5, 0, 1)
				end
			</OnMouseUp>
			<OnLeave>
				self.enter = false
				self:SetBackdropBorderColor(0, 0, 0, 0)
			</OnLeave>
			<OnDragStart>
				local parent = self:GetParent()
				self:SetBackdropBorderColor(0, 0, 0, 0)
				--parent:StartMoving()
				parent.isMoving = true
			</OnDragStart>
			<OnDragStop>
				self:SetScript("OnUpdate", nil)
				local parent = self:GetParent()
				--parent:StopMovingOrSizing()
				parent.isMoving = nil
				parent:SavePosition(parent)
				
				if self.enter then
					self:SetBackdropBorderColor(0.5, 0.5, 0, 1)
				end
			</OnDragStop>
		</Scripts>
	</Frame>
	
